$('#<%=@user.id%>name').html("<%= j (render partial: 'hotels/name', locals: { user: @user}) %>");
$('#<%=@user.id%>email').html("<%= j (render partial: 'hotels/email', locals: { user: @user}) %>");
$('#<%=@user.id%>phone').html("<%= j (render partial: 'hotels/phone', locals: { user: @user}) %>");
$('#<%=@user.id%>image').html("<%= j (render partial: 'hotels/image', locals: { user: @user}) %>");
<% if @alert %>
  alert("<%=@alert%>");
<%end%>